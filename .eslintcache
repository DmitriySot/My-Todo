nst [doneCount, undoneCount] = React.useMemo(()=> {\n      const done = todoData.reduce((sum, { done})=> {\n      return done ? sum+1  : sum\n      }, 0)\n      console.log(\"__sum__\", 1)\n      return [done, todoData.length-done]\n    },[stringtodoData])\n\n    const onTogglePosition = (id: number, direction: string) => {\n      const copyTodo = [...todoData]\n      const currentId = copyTodo.findIndex((item) => {\n       return item.id === id\n      })\n\n       const chunkTodo = copyTodo.splice(currentId, 1)\n       copyTodo.splice(direction === \"up\" ? currentId-1 : currentId+1,0,  ...chunkTodo )\n       setTodoData(copyTodo)\n\n\n    }\n\n    return (\n    <div className=\"app\">\n\n      <LoginBox />\n      <Header active={undoneCount} done={doneCount}/>\n      <div className=\"searchAndFilter\">\n        <SearchPanel onSearch={setTodoSearch}/>\n        <ItemStatusFilter onFilter={setTodoFilter} activeFilter={todoFilter}/>\n      </div>\n      <TodoList todos={filteredData}\n                onDelete={onDeleteItem}\n                onToggleStatus={onToggleStatus}\n                onTogglePosition={onTogglePosition}\n      />\n      <AddItem onAddItem={addNewItem} />\n\n    </div>\n  )\n}\n\nexport default App;\n",["64","65"],"E:\\react\\todotypescript\\src\\components\\ItemStatusFilter\\index.tsx",[],"E:\\react\\todotypescript\\src\\components\\SearchPanel\\index.tsx",[],["66","67"],"E:\\react\\todotypescript\\src\\components\\AddItem\\index.tsx",[],"E:\\react\\todotypescript\\src\\components\\LoginBox\\index.tsx",["68"],"import React from 'react'\r\nimport Modal from '../Modal'\r\nimport './style.css'\r\n\r\nconst LoginBox = () => {\r\n  return (\r\n\r\n      <div className='loginBox'>\r\n\r\n        <button className=\"btn btn-outline-secondary\" >Sign In</button>\r\n        <button className=\"btn btn-outline-secondary\">Creat Account</button>\r\n      </div>\r\n    \r\n  )\r\n}\r\n\r\nexport default LoginBox\r\n","E:\\react\\todotypescript\\src\\component